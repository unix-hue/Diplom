<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.e.projectmanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjectManager">
        <activity android:name=".TaskCreationActivity" android:label="@string/task_creation"/>
        <activity
            android:name=".TaskActivity"
            android:label="@string/task" />
        <activity
            android:name=".TasksActivity"
            android:label="@string/tasks" />
        <activity
            android:name=".DeskCreationActivity"
            android:label="@string/creation_desk" />
        <activity
            android:name=".DeskActivity"
            android:label="@string/desks" />
        <activity
            android:name=".AuthActivity"
            android:label="@string/auth"
            android:noHistory="true" />
        <activity
            android:name=".RegActivity"
            android:label="@string/registration"
            android:noHistory="true" />
        <activity
            android:name=".MainActivity"
            android:label="@string/registration"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>